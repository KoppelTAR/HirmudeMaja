@model IEnumerable<HirmudeMaja.Models.Seikleja>

@{
    ViewData["Title"] = "Majas Olevad Seiklejad";
}

<h1>MajasOlevadSeiklejad</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Eesnimi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sisenemisaeg)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Eesnimi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sisenemisaeg)
            </td>
        </tr>
}
    </tbody>
</table>
